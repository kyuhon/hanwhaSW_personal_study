<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.example.customer.mapper.CustomerMapper">

    <select id="findAll" resultType="org.example.customer.domain.CustomerSelect">
        SELECT customer_id AS customerId,
               name,
               email,
               phone
        FROM customers
        ORDER BY customer_id DESC
    </select>

</mapper>
